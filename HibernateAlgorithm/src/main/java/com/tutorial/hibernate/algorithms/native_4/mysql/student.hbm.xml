<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tutorial.hibernate.algorithms.domain.StudentBean"
		table="student">
		<id name="sid">
			<generator class="native" />
		</id>
		<property generated="never" lazy="false" name="sname" />
		<property generated="never" lazy="false" name="tot_m" />
	</class>
</hibernate-mapping>
